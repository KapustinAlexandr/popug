{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "server-url": "http://auth:8080/keycloak",
    "audience": "account",
    "name_claim": "preferred_username",
    "validate-issuer": true,
    "require-https": false
  }
}
